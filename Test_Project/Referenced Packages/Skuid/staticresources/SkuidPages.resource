n      </mobilepanels>\n      <mobilenavs>\n        <mobilenav uniqueid=\"header\">\n          <components>\n            <mobilegrid precision=\"6\" minwidth=\"50\">\n              <divisions>\n                <division size=\"1\">\n                  <components>\n                    <mobilebutton label=\"Menu\" style=\"nav\" icon=\"fa-bars\">\n                      <interactions>\n                        <interaction type=\"tap\">\n                          <actions>\n                            <action type=\"toggleLeftPanel\" panel=\"LeftNav\"/>\n                          </actions>\n                        </interaction>\n                      </interactions>\n                    </mobilebutton>\n                  </components>\n                </division>\n                <division size=\"4\">\n                  <components>\n                    <mobiletitle>\n                      <contents>Skuid Mobile Pages</contents>\n                    </mobiletitle>\n                  </components>\n                </division>\n                <division size=\"1\">\n                  <components>\n                    <mobilebutton label=\"View all Pages\" style=\"nav\" icon=\"fa-th\">\n                      <interactions>\n                        <interaction type=\"tap\">\n                          <actions>\n                            <action type=\"redirect\" url=\"/apex/skuid__PageList\"/>\n                          </actions>\n                        </interaction>\n                      </interactions>\n                    </mobilebutton>\n                  </components>\n                </division>\n              </divisions>\n            </mobilegrid>\n          </components>\n          <interactions>\n            <interaction type=\"doubletap\">\n              <actions>\n                <action type=\"toggleSetupMenu\"/>\n              </actions>\n            </interaction>\n            <interaction type=\"swipedown\">\n              <actions>\n                