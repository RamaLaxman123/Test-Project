false\" precision=\"1\" minwidth=\"400\" model=\"SelectedPage\" mode=\"edit\" emptysearchbehavior=\"query\">\n              <components>\n                <mobilefield id=\"Name\"/>\n                <mobilegrid precision=\"1\" minwidth=\"200\">\n                  <divisions>\n                    <division size=\"1\">\n                      <components>\n                        <mobilebutton label=\"Compose\" style=\"nav\" icon=\"fa-wrench\">\n                          <actions>\n                            <action type=\"redirect\" url=\"/apex/skuid__PageBuilder?id={{{Id}}}\" window=\"self\"/>\n                          </actions>\n                          <renderconditions logictype=\"and\"/>\n                          <enableconditions logictype=\"and\"/>\n                        </mobilebutton>\n                        <mobilebutton label=\"Preview\" style=\"nav\" icon=\"fa-flask\">\n                          <actions>\n                            <action type=\"custom\" url=\"\" snippet=\"previewPage\" event=\"skuidMobilePreviewPage\"/>\n                          </actions>\n                        </mobilebutton>\n                        <mobilebutton label=\"Clone\" style=\"nav\" icon=\"fa-copy\">\n                          <actions>\n                            <action type=\"setCondition\" model=\"ClonedPage\" condition=\"Name\" value=\"{{Name}}_clone\"/>\n                            <action type=\"setCondition\" model=\"ClonedPage\" condition=\"skuid__Layout__c\" value=\"{{{skuid__Layout__c}}}\"/>\n                            <action type=\"setCondition\" model=\"ClonedPage\" condition=\"skuid__Layout2__c\" value=\"{{{skuid__Layout2__c}}}\"/>\n                            <action type=\"setCondition\" model=\"ClonedPage\" condition=\"skuid__Layout3__c\" value=\"{{{skuid__Layout3__c}}}\"/>\n                            <action type=\"setCondition\" model=\"ClonedPage\" condition=\"skuid__Layout4__c\" value=\"{{{skuid__Layout4__c}}}\"/>\n                       